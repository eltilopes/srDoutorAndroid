<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true"   android:drawable="@drawable/icon_data" >
        <shape>

            <stroke
                android:width="1px" />

            <padding
                android:bottom="2dp"
                android:left="1dp"
                android:right="1dp"
                android:top="2dp" />

            <corners android:topLeftRadius="5dp" android:topRightRadius="5dp"
                android:bottomLeftRadius="0.2dp" android:bottomRightRadius="0.2dp"/>
        </shape>
    </item>
    <item android:state_enabled="false"    android:drawable="@drawable/icon_data" >
    <shape>

        <stroke
            android:width="1px" />

        <padding
            android:bottom="2dp"
            android:left="1dp"
            android:right="1dp"
            android:top="2dp" />
        <corners android:topLeftRadius="5dp" android:topRightRadius="5dp"
            android:bottomLeftRadius="0.2dp" android:bottomRightRadius="0.2dp"/>
    </shape>
</item>
</selector>